{
  "name": "axis-discovery-bonjour",
  "version": "1.0.0",
  "description": "A Node.js Bonjour client library written in TypeScript capable of searching for Axis Communication cameras.",
  "keywords": [
    "axis",
    "communications",
    "camera",
    "device",
    "discovery",
    "bonjour",
    "zero-configuration"
  ],
  "homepage": "https://github.com/FantasticFiasco/axis-discovery-bonjour",
  "repository": {
    "type": "git",
    "url": "https://github.com/FantasticFiasco/axis-discovery-bonjour.git"
  },
  "bugs": {
    "url": "https://github.com/FantasticFiasco/axis-discovery-bonjour/issues"
  },
  "author": "Mattias Kindborg <mattias.kindborg@gmail.com> (https://twitter.com/FantasticFiasco)",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "types": "./lib/index",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf \"./+(src|test)/**/*.+(d.ts|js|js.map)\" ./lib ./coverage",
    "prebuild": "yarn clean",
    "build": "tsc",
    "postbuild": "cpx \"./src/**/*.{d.ts,js}\" ./lib/",
    "prestart": "yarn build",
    "start": "node ./src/server.js",
    "pretest": "yarn build",
    "test": "mocha --recursive",
    "lint": "tslint \"./src/**/*.ts\" --exclude \"./**/*.d.ts\"",
    "precoverage": "yarn build",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec --recursive",
    "postcoverage": "remap-istanbul --input coverage/coverage.json --type lcovonly --output coverage/lcov.info",
    "precoverage:publish": "yarn coverage",
    "coverage:publish": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepack": "yarn test && yarn lint"
  },
  "dependencies": {
    "@fantasticfiasco/expect": "1.*",
    "bonjour": "FantasticFiasco/bonjour#66851c5",
    "debug": "4.*"
  },
  "devDependencies": {
    "@types/bonjour": "3.5.5",
    "@types/chai": "4.1.7",
    "@types/debug": "0.0.31",
    "@types/mocha": "5.2.5",
    "@types/node": "10.11.3",
    "@types/sinon": "5.0.2",
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "cpx": "1.5.0",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "remap-istanbul": "0.12.0",
    "rimraf": "2.6.2",
    "sinon": "6.3.4",
    "tslint": "5.11.0",
    "typescript": "3.1.1"
  }
}