{
  "name": "axis-discovery-bonjour",
  "version": "1.0.8",
  "description": "A Node.js Bonjour client library written in TypeScript capable of searching for Axis Communication cameras.",
  "keywords": [
    "axis",
    "communications",
    "camera",
    "device",
    "discovery",
    "bonjour",
    "zero-configuration"
  ],
  "homepage": "https://github.com/FantasticFiasco/axis-discovery-bonjour-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FantasticFiasco/axis-discovery-bonjour-js.git"
  },
  "bugs": {
    "url": "https://github.com/FantasticFiasco/axis-discovery-bonjour-js/issues"
  },
  "author": "Mattias Kindborg <mattias.kindborg@gmail.com> (https://twitter.com/FantasticFiasco)",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "types": "./lib/index",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf \"./+(src|test)/**/*.+(d.ts|js|js.map)\" ./lib ./coverage",
    "prebuild": "yarn clean",
    "build": "tsc",
    "postbuild": "copyfiles --up 1 \"./src/**/*.{d.ts,js}\" ./lib/",
    "prestart": "yarn build",
    "start": "node ./src/server.js",
    "test": "jest",
    "lint": "tslint \"./src/**/*.ts\" --exclude \"./**/*.d.ts\"",
    "coverage": "jest --coverage && coveralls < coverage/lcov.info",
    "merge-dependencies": "node ./build/merge-dependencies.js"
  },
  "dependencies": {
    "@fantasticfiasco/expect": "1.*",
    "array-flatten": "^2.1.0",
    "debug": "4.*",
    "deep-equal": "^1.0.1",
    "dns-equal": "^1.0.0",
    "dns-packet": "^1.0.1",
    "dns-txt": "^2.0.2",
    "multicast-dns-service-types": "^1.1.0",
    "thunky": "^0.1.0"
  },
  "devDependencies": {
    "@types/bonjour": "3.5.6",
    "@types/debug": "4.1.5",
    "@types/jest": "25.1.5",
    "@types/node": "13.11.1",
    "copyfiles": "2.2.0",
    "coveralls": "3.0.11",
    "jest": "25.2.6",
    "rimraf": "3.0.2",
    "ts-jest": "25.3.0",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  },
  "resolutions": {
    "diff": ">=3.5.0",
    "handlebars": ">=4.3.0",
    "js-yaml": ">=3.13.0",
    "lodash": ">=4.17.13",
    "minimist": ">=1.2.2",
    "sshpk": ">=1.13.2"
  }
}
